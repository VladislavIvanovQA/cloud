<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="202.0" prefWidth="317.0"
            xmlns="http://javafx.com/javafx/11.0.2" fx:controller="org.cloud.client.controllers.AuthController">
    <children>
        <Label layoutX="21.0" layoutY="25.0" text="Логин:" AnchorPane.bottomAnchor="160.0" AnchorPane.leftAnchor="21.0"
               AnchorPane.rightAnchor="259.0" AnchorPane.topAnchor="25.0"/>
        <Label layoutX="18.0" layoutY="82.0" text="Пароль:" AnchorPane.leftAnchor="18.0" AnchorPane.topAnchor="82.0"/>
        <TextField fx:id="loginField" layoutX="87.0" layoutY="21.0"/>
        <PasswordField fx:id="passwordField" layoutX="87.0" layoutY="78.0"/>
        <Button fx:id="authButton" layoutX="84.0" layoutY="129.0" mnemonicParsing="false" onAction="#executeAuth"
                prefHeight="25.0" prefWidth="149.0" text="Войти!"/>
        <Button fx:id="registrationButton" layoutX="84.0" layoutY="163.0" mnemonicParsing="false"
                onAction="#registration" prefHeight="25.0" prefWidth="149.0" text="Регистрация"/>
    </children>
</AnchorPane>
