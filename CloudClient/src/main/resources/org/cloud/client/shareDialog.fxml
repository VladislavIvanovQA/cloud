<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="167.0" prefWidth="317.0"
            xmlns="http://javafx.com/javafx/11.0.2" fx:controller="org.cloud.client.controllers.ShareController">
    <children>
        <Label layoutX="21.0" layoutY="25.0" text="File:" AnchorPane.bottomAnchor="160.0" AnchorPane.leftAnchor="21.0"
               AnchorPane.rightAnchor="259.0" AnchorPane.topAnchor="25.0"/>
        <Label layoutX="21.0" layoutY="93.0" prefHeight="17.0" prefWidth="62.0" text="Expire link"
               AnchorPane.leftAnchor="21.0" AnchorPane.topAnchor="93.0"/>
        <Label layoutX="21.0" layoutY="41.0" prefHeight="47.0" prefWidth="80.0" text="Sing download?" wrapText="true"
               AnchorPane.leftAnchor="21.0" AnchorPane.topAnchor="41.0"/>
        <Button fx:id="shareBtn" layoutX="29.0" layoutY="131.0" mnemonicParsing="false" onAction="#shareFileAction"
                prefHeight="25.0" prefWidth="265.0" text="Share file"/>
        <Label fx:id="pathLabel" layoutX="103.0" layoutY="25.0" prefHeight="17.0" prefWidth="149.0" text="filename"/>
        <DatePicker fx:id="datepicker" layoutX="101.0" layoutY="89.0"/>
        <CheckBox fx:id="singDownload" layoutX="101.0" layoutY="56.0" mnemonicParsing="false"/>
        <Button fx:id="closeBtn" layoutX="29.0" layoutY="163.0" mnemonicParsing="false" onAction="#closeAction"
                prefHeight="25.0" prefWidth="265.0" text="Close"/>
    </children>
</AnchorPane>
